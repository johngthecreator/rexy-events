{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": ["ts-jest", {
        "tsconfig": "tsconfig.json"
      }]
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testMatch": ["**/?(*.)+(spec|test).(ts|tsx|js)"]
  }
  